{
    "name": "mrgoodbytes8667/test-common",
    "type": "library",
    "description": "Common helper traits for PHPUnit with Symfony",
    "license": "CC-BY-NC-4.0",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "doctrine/common": "^3.0",
        "illuminate/collections": "^8.0",
        "mrgoodbytes8667/enum-serializer-bundle": "^1.2.3 | ^2.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "phpunit/phpunit": "*",
        "symfony/http-client": "^5.2 | ^6.0",
        "symfony/phpunit-bridge": "^5.2 | ^6.0",
        "symfony/property-info": "^5.2 | ^6.0",
        "symfony/validator": "^5.2 | ^6.0",
        "symfony/var-dumper": "^5.2 | ^6.0"
    },
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@goodbytes.live"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "conflict": {
        "doctrine/annotations": "<1.11.1",
        "doctrine/cache": "<1.10.2",
        "doctrine/collections": "<1.6.7",
        "doctrine/event-manager": "<1.1.1",
        "doctrine/inflector": "<1.4.3",
        "doctrine/lexer": "<1.2.1",
        "doctrine/persistence": "<2.1.0"
    },
    "autoload": {
        "psr-4": {
            "Bytes\\Tests\\Common\\": "Tests/"
        }
    },
    "minimum-stability": "dev"
}
